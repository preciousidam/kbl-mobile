{
  "expo": {
    "name": "KBL Insurance",
    "slug": "kbl-insurance",
    "version": "1.0.0",
    "scheme": "kblapp",
    "orientation": "portrait",
    "icon": "./assets/kblicon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.kblinsurance",
      "supportsTablet": true,
      "googleServicesFile": "./google-services.json",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to let user put a photo in his profile page.",
        "NSLocationWhenInUseUsageDescription": "This app needs to access location to show current location on map"
      }
    },
    "android": {
      "package": "com.kblinsurance",
      "versionCode": 2,
      "enableDangerousExperimentalLeanBuilds": true,
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleSignIn": {
          "apiKey": "AIzaSyDxVTNNv2HLzYiIbWGUUhlsGh3ATjxz74w"
        }
      },
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA_ROLL",
        "READ_EXTERNAL_STORAGE",
        "MEDIA_LIBRARY",
        "MEDIA_LIBRARY_WRITE_ONLY",
        "USE_FINGERPRINT",
        "USE_BIOMETRIC",
        "VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "KBL Insurance mobile app",
    "notification": {
      "icon": "./assets/kblicon.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true
    }
  }
}
